{
  "name": "dejavu-cli",
  "version": "0.0.2",
  "description": "A powerful CLI tool for web libraries like React, simplifying component creation and enabling seamless development.",
  "main": "index.js",
  "bin": {
    "dejavu": "index.js",
    "vu": "index.js"
  },
  "scripts": {
    "build": "rimraf dist/ && bun build --target=node --outfile=index.js --outdir=dist --minify src/index.ts",
    "dev": "bun src/index.ts",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage"
  },
  "author": "Jean Pierre Ortiz",
  "license": "ISC",
  "dependencies": {
    "bun": "^1.2.16",
    "chalk": "^4.1.1",
    "commander": "^11.1.0",
    "glob": "^11.0.3",
    "inquirer": "^8.2.6"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/chalk": "^2.2.4",
    "@types/commander": "^2.12.5",
    "@types/inquirer": "^8.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.4.8",
    "babel-jest": "^30.0.2",
    "jest": "^30.0.2",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.4.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CoffeJeanCode/dejavu-cli"
  },
  "bugs": {
    "url": "https://github.com/CoffeJeanCode/dejavu-cli/issues"
  },
  "homepage": "https://github.com/CoffeJeanCode/dejavu-cli#readme",
  "private": true
}